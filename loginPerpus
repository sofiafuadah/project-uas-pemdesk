    def check_password(self):
        msg = QMessageBox()
        if self.username.text() == 'admin' and self.password.text() == '123456':
            Index =  TampilanIndex()
            Index.show() 
        else:
            msg.setText("Incorrect Username or Password")
            msg.exec()



